    "@graphql-tools/stitch": "^9.0.0",
    "@graphql-tools/utils": "^9.2.1",
    "@graphql-tools/wrap": "^9.3.5",
    "apollo-server-plugin-response-cache": "^3.8.2",
    "async-mutex": "^0.4.0",
    "basic-auth": "^2.0.1",
