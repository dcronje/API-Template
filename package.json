    "lru-cache": "^7.18.3",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "moment-business-days": "^1.2.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "node-env-file": "^0.1.8",
    "node-notifier": "^10.0.1",
    "nodemailer": "^6.9.1",
