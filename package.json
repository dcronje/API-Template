{
  "name": "silvertree-wms",
  "version": "1.0.0",
  "main": "build/server",
  "author": "Drew Cronje",
  "license": "MIT",
  "scripts": {
    "build": "npm run peon:generate && npx tsc --project ./tsconfig.json",
    "clean": "npx trash ./build",
    "dev": "npx gulp dev",
    "cluster": "npx gulp cluster",
    "queues": "npx gulp queues",
    "start": "NODE_ENV=production node --max-old-space-size=32768 -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts --project ./tsconfig.json",
    "typeorm": "npx ts-node --transpile-only --files -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "sync": "npx node -r module-alias/register /app/node_modules/typeorm/cli.js schema:sync",
    "migrate:generate": "npx ts-node --transpile-only --files -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts -n ./src/migrations",
    "migrate:up": "npx node -r module-alias/register /app/node_modules/typeorm/cli.js migration:run",
    "migrate:down": "npx node -r module-alias/register /app/node_modules/typeorm/cli.js migration:revert",
    "peon": "npx ts-node -r tsconfig-paths/register --transpile-only --project ./tsconfig.json ./peon/index.ts",
    "peon:clean": "npx trash ./src/generated && mkdir ./src/generated && touch ./src/generated/.empty",
    "peon:generate": "npx ts-node -r tsconfig-paths/register --transpile-only --project ./tsconfig.json ./peon/index.ts --generate",
    "import": "node --max-old-space-size=32768 -r ts-node/register/transpile-only -r tsconfig-paths/register src/importer.ts --project ./tsconfig.json",
    "test": "npm run peon:generate && npx --max-old-space-size=32768 tsc --project ./tsconfig.json --noEmit",
    "test:once": "npx jest --runInBand --verbose --env=node",
    "test:watch": "npx jest --runInBand --verbose --env=node --watch",
    "test:coverage": "npx jest --runInBand --env=node --coverage",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
